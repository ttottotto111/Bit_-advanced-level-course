<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <system.serviceModel>
    <services>
      <!--네임스페이스명을 포함한 서비스 객체-->
      <service name="_1124.HelloWorldWCFService"
               behaviorConfiguration="1111111">
        <host>
          <baseAddresses>
            <add baseAddress="http://localhost:8080/wcf/example/helloworldservice%22/%3E"/>
          </baseAddresses>
        </host>
        <endpoint
            contract ="_1124.IHelloWorld"
            address =""
            binding ="basicHttpBinding"
                    />
      </service>
    </services>
    <behaviors>
      <serviceBehaviors>
        <behavior name="1111111">
          <serviceMetadata httpGetEnabled="true"/>
        </behavior>
      </serviceBehaviors>
    </behaviors>
  </system.serviceModel>
</configuration>